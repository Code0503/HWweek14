<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testsuite" verbose="1">
    <listeners>
        <listener class-name="com.parabank.demo.customlisteners.CustomListeners"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="Register and Login Test">
        <classes>
            <class name="com.parabank.demo.testsuite.RegisterTest"/>
            <class name="com.parabank.demo.testsuite.LoginTest"/>
        </classes>
    </test>
</suite>